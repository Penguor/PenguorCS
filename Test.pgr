extern void printf();

void main() {
    FizzBuzz(100)
}

void FizzBuzz(i32 count) {
    i32 i = 1
    while(i <= count) {
        bool fizzed = false
        if(i % 3 == 0) {
            printf("Fizz")
            fizzed = true
        }
        if(i % 5 == 0) {
            printf("Buzz")
            fizzed = true
        }

        if(!fizzed) {
            printNum(i)
        }

        println()
        i += 1
    }
}

void printNum(i32 num) {
    printf("%d", num)
}

void println() {
    printf("\n")
}