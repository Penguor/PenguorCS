void main() {
    FizzBuzz(100)
}

void FizzBuzz(i32 count) {
    i32 i = 1
    while(i <= count) {
        bool fizzed = false
        if(i % 3 == 0) {
            print("Fizz")
            fizzed = true
        }
        if(i % 5 == 0) {
            print("Buzz")
            fizzed = true
        }

        if(!fizzed) {
            printNum(i)
        }

        println()

        i++
    }
}

void printNum(i32 num) {
    print("%d", num)
}

void println() {
    print("\n")
}

