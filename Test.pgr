void main() {
    FizzBuzz(100)
}

void FizzBuzz(i32 count) {
    i32 i = 1
    while(i < count) {
        bool fizzed = false
        if(i % 3 == 0) {
            print("Fizz")
            fizzed = true
        }
        if(i % 5 == 0) {
            print("Buzz")
            fizzed = true
        }

        if(!fizzed) {
            printItoa(i)
        }

        println()

        i += 1
    }
}

void printItoa(i32 i) {
    while(i > 0) {
        i32 c = i % 10
        i /= 10

        print(i + '0')
    }
}

