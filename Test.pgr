    // returns the fibonacci number at n
    private int getFibonacci(int n)
    {
        if(n == 0)
            return 0
        else if(n == 1)
            return 1
        else
            return getFibonacci(n - 1) + getFibonacci(n - 2)
    }

    // like the one above, but using a different algorithm
    private int fibMoivreBinet(int n)
    {
        return (
            (1 / 5) * (
                (pow(
                    (1 + sqrt(5)
                ) / 2, n))
                 -
                (pow(
                    (1 - sqrt(5)
                ) / 2, n))
            ))
    }
}